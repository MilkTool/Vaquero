
(def r1 \x)
(def r2 \3)
(def r3 \lf)
(def R1 \X)

(test 'type (= 'rune r1.type))
(test 'alpha? (and r1.alpha? r2.alpha?.not r3.alpha?.not))
(test 'digit? (and r1.digit?.not r2.digit? r3.digit?.not))
(test 'whitespace? (and r1.whitespace?.not r2.whitespace?.not r3.whitespace?))
(test 'uc? (and r1.uc?.not R1.uc?))
(test 'lc? (and r1.lc? R1.lc?.not))
(test 'uc (= r1.uc R1))
(test 'lc (= r1 R1.lc))
(test 'to-bool (= true r3.to-bool))
(test 'to-text (= "x" r1.to-text))

