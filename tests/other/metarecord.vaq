
(proc meta (n rec)
    (if (= n 0)
        rec
        (seq
            (rec.set! (gensym) (table))
            (meta (- n 1) rec))))

(proc meta-meta (x)
    (meta 1000 x)
    (meta 1000 x)
    (meta 1000 x)
    (meta 1000 x)
    (meta 1000 x))

(def x (table))

(meta-meta x)
