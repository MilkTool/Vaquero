
(def p (pair 'x 3))

(test pair-type     (pair? p))
(test pair-to-bool  p.to-bool)
(test pair-head     (= p.head 'x))
(test pair-tail     (= p.tail 3))
(test pair-size     (= p.size 2))

(test pair-head!
   (= '(b . 1)
      (let (p2 (pair 'a 1))
         (p2.head! 'b)
         p2)))

(test pair-tail!
   (= '(a . 2)
      (let (p2 (pair 'a 1))
         (p2.tail! '2)
         p2)))

