(test list-head!
   (= '(b 1)
      (let (L (list 'a 1))
         (L.head! 'b)
         L)))

(test list-tail!
   (= '(a . 2)
      (let (L (list 'a 1))
         (L.tail! '2)
         L)))

(test list-set!
   (= '(1 2 72)
      (let (L (list 1 2 3))
         (L.set! 2 72)
         L)))

